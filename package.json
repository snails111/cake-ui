{
  "name": "cake-ui-w",
  "version": "4.0.16",
  "description": "基于antd 4.0版本二次开发cake-ui组件库",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "scripts": {
    "clean:commonjs": "rimraf lib",
    "clean:umd": "rimraf dist",
    "clean:esm": "rimraf esm",
    "clean:exdist": "rimraf doc/dist",
    "clean:coverage": "rimraf coverage",
    "clean": "npm-run-all clean:*",
    "create-github-release": "conventional-github-releaser -p angular",
    "build:commonjs": "cross-env BABEL_OUTPUT=commonjs babel src/ --out-dir lib/ --ignore **/__tests__,**/__mocks__",
    "build:esm": "babel src/ --out-dir esm/ --ignore **/__tests__,**/__mocks__",
    "build:umd": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "build": "npm-run-all clean:* --parallel build:*",
    "format": "eslint src --fix",
    "lint": "eslint src",
    "prebump": "run-s lint test",
    "release": "standard-version --sign",
    "test": "cross-env BABEL_OUTPUT=commonjs jest",
    "codecov": "codecov",
    "startDoc": "webpack-dev-server --inline --content-base doc/ --config webpack.config.doc.js",
    "buildDoc:commonjs": "cross-env NODE_ENV=production webpack --config webpack.config.doc.js",
    "website": "run-s website:clean website:build",
    "website:clean": "rimraf doc/dist",
    "website:build": "cross-env NODE_ENV=production webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lcwebgroup/cake-ui.git"
  },
  "homePage": {
    "url": "https://lcwebgroup.github.io/cake-ui/doc/dist/index.html#/"
  },
  "author": "crisBai",
  "license": "MIT",
  "files": [
    "dist",
    "esm",
    "lib",
    "style"
  ],
  "keywords": [
    "react",
    "component",
    "template",
    "react-component"
  ],
  "peerDependencies": {
    "react": "^0.14.9 || ^15.3.0 || ^16.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/plugin-external-helpers": "^7.0.0-beta.40",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.40",
    "@babel/plugin-proposal-export-default-from": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.40",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.0.0-beta.40",
    "@rollup/plugin-commonjs": "^12.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^22.2.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.0",
    "babel-preset-stage-0": "^6.24.1",
    "codecov": "^3.0.0",
    "conventional-github-releaser": "^2.0.0",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.0",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.1",
    "eslint": "^4.5.0",
    "eslint-config-airbnb": "^16.0.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-plugin-react": "^7.0.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "hoist-non-react-statics": "^2.3.1",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^4.0.0-beta.14",
    "husky": "^0.14.3",
    "jest": "^22.3.0",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "lint-staged": "^6.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "moment": "^2.25.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.2.2",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-test-renderer": "^16.0.0",
    "rimraf": "^2.5.2",
    "rollup": "^0.56.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-ignore": "^1.0.3",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^3.0.0",
    "standard-version": "^4.2.0",
    "style-loader": "^0.20.0",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.5.1"
  },
  "dependencies": {
    "@ant-design/icons": "^4.1.0",
    "@antv/g2plot": "^1.1.2",
    "antd": "^4.2.0",
    "classnames": "^2.2.0",
    "prop-types": "^15.5.0",
    "react-loadable": "^5.5.0",
    "react-router-dom": "^5.1.2",
    "react-syntax-highlighter": "^12.2.1"
  },
  "jest": {
    "roots": [
      "src"
    ],
    "collectCoverage": true,
    "testRegex": "/__tests__/.+-test\\.js$"
  }
}
